from oblif.decorator import oblif
from oblif.iterators import orange
from oblif.compat import patch_mpspdz_type
patch_mpspdz_type(sint)

@oblif
def test(x):
  if x<3:
    ret = 0
  elif 3<=x<5:
    ret = 1
  else:
    ret = 2
  return ret
  
for i in range(8): print_ln("test(%s) is %s", i, test(sint(i)).reveal())

